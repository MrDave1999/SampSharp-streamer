<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Title>Streamer Wrapper for SampSharp.Entities</Title>
    <Description>A SampSharp wrapper for Incognito's streamer plugin.</Description>
    <PackageId>SampSharp.CTF.Streamer.Entities</PackageId>
    <Product>$(PackageId)</Product>
    <Version>0.10.1</Version>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="SampSharp.CTF.Entities" Version="0.10.6" />
  </ItemGroup>

  <ItemGroup>
    <None Include=".\README.md" Pack="True" PackagePath="" />
  </ItemGroup>

</Project>